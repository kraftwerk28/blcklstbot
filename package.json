{
  "name": "blcklstbot",
  "version": "2.0.1",
  "main": "index.js",
  "author": "Vsevolod (@kraftwerk28) <kefirchik3@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -r dist/*",
    "build:test": "tsc --outDir test/dist/ --esModuleInterop true test/db.test.ts",
    "deploy-db": "psql -d blcklstbot -f ./db.sql",
    "dev": "NODE_ENV=development node build/",
    "format": "prettier --ignore-path .gitignore --write src tests migrations",
    "lint": "eslint --ignore-path .gitignore src",
    "lint:fix": "npm run lint -- --fix",
    "nodemon": "NODE_ENV=development nodemon build/",
    "test": "NODE_ENV=development ts-node test/index.test.ts",
    "watch": "tsc -w --inlineSourceMap"
  },
  "dependencies": {
    "dotenv": "^9.0.2",
    "ioredis": "^4.27.1",
    "knex": "^0.95.4",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "pino": "^7.11.0",
    "telegraf": "^4.3.0"
  },
  "devDependencies": {
    "@types/ioredis": "^4.19.3",
    "@types/node": "^18.0.3",
    "@types/node-fetch": "^2.5.7",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.8.1",
    "ts-node": "^8.10.2",
    "typegram": "^3.1.4",
    "typescript": "^4.1.3"
  }
}
